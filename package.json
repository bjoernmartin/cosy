{
	"name": "cosy",
	"version": "1.54.4",
	"private": true,
	"author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
	"license": "Apache-2.0",
	"scripts": {
		"lint": "eslint src",
		"predev": "rimraf out",
		"dev": "concurrently 'npm:dev:*'",
		"dev:server": "NODE_ENV=development onchange -i -k 'src/server/**/*.js' -- node src/server/index.js",
		"dev:browser": "NODE_ENV=development onchange -i -k 'src/browser/**/*.js' -- rollup -c",
		"dev:css": "NODE_ENV=development postcss src/browser/**/*.css --base src/browser --dir out/css",
		"prebuild": "rimraf out",
		"build": "concurrently 'npm:build:*'",
		"build:js": "rollup -c",
		"build:css": "postcss src/browser/**/*.css --base src/browser --dir out/css",
		"format": "prettier --write src"
	},
	"dependencies": {
		"debug": "4.2.0",
		"dotenv": "8.2.0",
		"glob": "7.1.6",
		"handlebars": "4.7.6",
		"koa": "2.13.0",
		"koa-bodyparser": "4.3.0",
		"koa-compress": "5.0.1",
		"koa-csrf": "3.0.8",
		"koa-helmet": "6.0.0",
		"koa-passport": "4.1.3",
		"koa-route": "3.2.0",
		"koa-send": "5.0.1",
		"koa-session": "6.1.0",
		"koa-static": "5.0.0",
		"koa-views": "6.3.1",
		"koa-websocket": "6.0.0",
		"passport-local": "1.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "15.1.0",
		"@rollup/plugin-node-resolve": "9.0.0",
		"@rollup/plugin-replace": "2.3.3",
		"concurrently": "5.3.0",
		"cssnano": "4.1.10",
		"eslint": "7.10.0",
		"eslint-config-prettier": "6.12.0",
		"eslint-plugin-prettier": "3.1.4",
		"onchange": "6.1.0",
		"postcss": "8.1.1",
		"postcss-cli": "8.1.0",
		"prettier": "2.1.2",
		"rimraf": "3.0.2",
		"rollup": "2.29.0",
		"tailwindcss": "1.8.12"
	}
}
