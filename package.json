{
	"name": "synyx-remote",
	"version": "0.3.0",
	"private": true,
	"author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
	"scripts": {
		"dev": "NODE_ENV=development concurrently \"npm:dev:server\" \"npm:dev:browser\"",
		"dev:server": "onchange -i -k 'src/server/**/*.js' -- node src/server/index.js",
		"dev:browser": "onchange -i -k 'src/browser/**/*.js' -- rollup -c",
		"css": "tailwind build src/css/style.css -o static/css/style.css",
		"build": "NODE_ENV=production rollup -c"
	},
	"dependencies": {
		"canvas": "2.6.1",
		"dotenv": "8.2.0",
		"glob": "7.1.6",
		"gravatar-url": "3.1.0",
		"handlebars": "4.7.6",
		"koa": "2.11.0",
		"koa-bodyparser": "4.3.0",
		"koa-csrf": "3.0.8",
		"koa-helmet": "5.2.0",
		"koa-passport": "4.1.3",
		"koa-route": "3.2.0",
		"koa-session": "5.13.1",
		"koa-static": "5.0.0",
		"koa-views": "6.2.1",
		"koa-websocket": "6.0.0",
		"passport-ldapauth": "2.1.4",
		"passport-local": "1.0.0"
	},
	"devDependencies": {
		"concurrently": "5.1.0",
		"eslint": "6.8.0",
		"eslint-config-prettier": "6.10.1",
		"eslint-plugin-prettier": "3.1.2",
		"onchange": "6.1.0",
		"postcss-cli": "7.1.0",
		"prettier": "2.0.4",
		"rollup": "2.7.2",
		"rollup-plugin-cleaner": "1.0.0",
		"rollup-plugin-terser": "5.3.0",
		"tailwindcss": "1.2.0"
	}
}
